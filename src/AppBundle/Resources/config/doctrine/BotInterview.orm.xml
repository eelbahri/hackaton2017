<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\BotInterview" table="bot_interview">
    <indexes>
      <index name="id_users" columns="id_users"/>
    </indexes>
    <id name="idBotInterview" type="integer" column="id_bot_interview">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="dateStart" type="datetime" column="date_start" nullable="false"/>
    <field name="dateEnd" type="datetime" column="date_end" nullable="false"/>
    <field name="isActive" type="boolean" column="is_active" nullable="false"/>
    <many-to-one field="idUsers" target-entity="Users" fetch="LAZY">
      <join-columns>
        <join-column name="id_users" referenced-column-name="id_users"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
