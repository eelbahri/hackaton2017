<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\AdminUsers" table="admin_users">
    <unique-constraints>
      <unique-constraint name="mail" columns="mail"/>
    </unique-constraints>
    <id name="idAdminUsers" type="integer" column="id_admin_users">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="fisrtName" type="string" column="fisrt_name" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="lastName" type="string" column="last_name" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="mail" type="string" column="mail" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="password" type="text" column="password" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="lastConnection" type="datetime" column="last_connection" nullable="false">
      <options>
        <option name="default">CURRENT_TIMESTAMP</option>
      </options>
    </field>
    <field name="lastAdress" type="integer" column="last_adress" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="isActive" type="boolean" column="is_active" nullable="false"/>
  </entity>
</doctrine-mapping>
