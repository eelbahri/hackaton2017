<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Users" table="users">
    <unique-constraints>
      <unique-constraint name="id_facebook" columns="id_facebook"/>
    </unique-constraints>
    <id name="idUsers" type="integer" column="id_users">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="lastName" type="string" column="last_name" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="firstName" type="string" column="first_name" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="idFacebook" type="string" column="id_facebook" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="phoneNumber" type="integer" column="phone_number" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="dateBorn" type="date" column="date_born" nullable="false"/>
    <field name="isActive" type="integer" column="is_active" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
