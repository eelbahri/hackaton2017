<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\ResponseRecruit" table="response_recruit">
    <indexes>
      <index name="id_users" columns="id_users"/>
      <index name="id_questions" columns="id_questions"/>
    </indexes>
    <id name="idResponseRecruit" type="integer" column="id_response_recruit">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="response" type="text" column="response" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="idQuestions" target-entity="QuestionsRecruit" fetch="LAZY">
      <join-columns>
        <join-column name="id_questions" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idUsers" target-entity="Users" fetch="LAZY">
      <join-columns>
        <join-column name="id_users" referenced-column-name="id_users"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
